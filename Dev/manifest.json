{
    "name": "Frog Login",
    "version": "0.2",
    "manifest_version": 2,
    "description": "Log into frog easily and quickly.",
    "options_page": "options.html",
    "icons": {
        "16": "icon.png",
        "48": "icon128.jpg",
        "128": "icon128.jpg"
    },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "details.html"
    },  
    "permissions": [
        "tabs",
        "*://*.frogosdev.co.uk/*"
    ]
}